  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "build": "SET NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration=local --output-hashing none && node concat.js",
    "build:dev": "SET NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration=development --output-hashing none && node concat.js",
    "build:prod": "SET NODE_OPTIONS=--openssl-legacy-provider && ng build --configuration=production --output-hashing none && node concat.js",
    "start": "http-server ./dist/mfe-tf-leaderboard -p 4102",
    "build:local": "SET NODE_OPTIONS=--openssl-legacy-provider && ng build",
    "start:local": "SET NODE_OPTIONS=--openssl-legacy-provider && ng serve --port 4400",
    "test": "SET NODE_OPTIONS=--openssl-legacy-provider && ng test"
